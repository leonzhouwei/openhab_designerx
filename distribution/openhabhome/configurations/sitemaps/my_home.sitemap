sitemap my_home {
    Frame {
        Group item=gFF label="First Floor" icon="1stfloor"
        Group item=gGF label="Ground Floor" icon="groundfloor"
        Group item=gC label="Cellar" icon="cellar"
        Group item=Outdoor icon="gardeon"
    }
    Frame label="Weather" {
        Text item=Weather_Temperature label="Weather" {
            Frame item=Weather_Temp_Max
            Frame item=Weather_Temp_Min
        }
    }
    Frame label="Date" {
        Text item=Date
    }
}